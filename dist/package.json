{
  "name": "simply.careers",
  "version": "1.0.0",
  "description": "",
  "main": "src/app.ts",
  "scripts": {
    "start": "cd build && node app.js",
    "setup": "cd docker && sudo docker-compose up",
    "ci:webapp": "npx webpack --env WEBAPP_ENV=dev --config webapp.webpack.js --progress",
    "ci:test:unit": "jest --bail --all --runInBand",
    "ci:test:browser": "npx testcafe -c 4 \"chrome:headless\"",
    "server":"npx nodemon --signal SIGTERM -e ts,tsx,js -w ./src/server -w ./src/api -x node -r ts-node/register -- ./src/app.ts",
    "webapp": "npm-run-all -p -n webapp:cleanup webapp:code webapp:styles:build webapp:styles:watch",
    "webapp:code": "npx webpack --env WEBAPP_ENV=dev --config webapp.webpack.js --progress --watch",
    "webapp:styles:watch": "npx node-sass src/webapp/scss/root.scss dist/public/styles/main.min.css -w --source-map true",
    "webapp:styles:build": "npx node-sass src/webapp/scss/root.scss dist/public/styles/main.min.css",
    "webapp:cleanup": "npx rimraf dist/build/webapp",
    "test": "npm-run-all -n ci:test:unit ci:test:browser",
    "test:unit": "jest --bail --runInBand --watch",
    "test:server": "jest --selectProjects server --bail --runInBand --watch",
    "test:browser": "npx testcafe -c 4 \"chrome:headless\" --live",
    "test:browser:win": "npx testcafe chrome --live",
    "lint": "npx eslint -c .eslintrc.js --ext .ts --ext .tsx ./src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/g-wozniak/simply.careers.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/g-wozniak/simply.careers/issues"
  },
  "homepage": "https://github.com/g-wozniak/simply.careers#readme",
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "axios": "^0.21.1",
    "cors": "^2.8.5",
    "eslint": "^7.22.0",
    "express": "^4.17.1",
    "express-static-gzip": "^2.1.1",
    "helmet": "^4.4.1",
    "is-port-reachable": "^3.0.0",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.2",
    "mongoose": "^5.12.1",
    "nodemon": "^2.0.7",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-meta-tags": "^1.0.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "ts-node": "^9.1.1",
    "tslib": "^2.1.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/testcafe": "^4.3.1",
    "@types/axios": "^0.14.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^26.0.21",
    "@types/lodash": "^4.14.168",
    "@types/mongodb-memory-server": "^2.3.0",
    "@types/mongoose": "^5.10.3",
    "@types/node": "^14.14.35",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "@types/react-redux": "^7.1.16",
    "@types/react-router-dom": "^5.1.7",
    "@types/redux": "^3.6.0",
    "@types/redux-thunk": "^2.1.0",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.3.0",
    "brotli-webpack-plugin": "^1.1.0",
    "compression-webpack-plugin": "^7.1.2",
    "eslint-plugin-react": "^7.23.1",
    "html-replace-webpack-plugin": "^2.6.0",
    "html-webpack-plugin": "^5.3.1",
    "jest": "^26.6.3",
    "jest-serial-runner": "^1.1.0",
    "mongodb-memory-server": "^6.9.6",
    "nanoid": "^3.1.22",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "promise": "^8.1.0",
    "supertest": "^6.1.3",
    "testcafe": "^1.12.0",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.0.18",
    "tscpaths": "0.0.9",
    "typescript": "^4.2.3",
    "webpack": "^5.26.3",
    "webpack-cli": "^4.5.0"
  },
  "_moduleAliases": {
    "@intf": "src/_intf",
    "@webapp": "src/webapp"
  }
}
